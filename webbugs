function onLoadActions() {
  quicksearchHelpText('quicksearch_main', 'show');
  if( window.external.AddSearchProvider ){
    YAHOO.util.Dom.removeClass('quicksearch_plugin', 'bz_default_hidden');
  }
  document.getElementById('quicksearch_top').focus();
}
function addSidebar() {
    var sidebarname=window.location.host;
    if (!/bug/i.test(sidebarname))
      sidebarname="Web Bug Tracker "+sidebarname;
    window.sidebar.addPanel (sidebarname, "http://developer.symbian.org/webbugs/sidebar.cgi", "");
}
var quicksearch_message = "Enter a bug # or some search terms";
function checkQuicksearch( form ) {
  if (form.quicksearch.value == '' || form.quicksearch.value == quicksearch_message ) { 
    alert('Please enter one or more search terms first.');
    return false; 
  }
  return true;         
}
function quicksearchHelpText(el_id, action){
  var el = document.getElementById(el_id);
  if ( action == "show") {
    if( el.value == "" ) {
      el.value = quicksearch_message
      YAHOO.util.Dom.addClass(el, "quicksearch_help_text");
    }
  } else {
    if( el.value == quicksearch_message ) {
      el.value = "";
      YAHOO.util.Dom.removeClass(el, "quicksearch_help_text");
    }
  }
}
YAHOO.util.Event.onDOMReady(onLoadActions);
//-->
</script>


<div id="page-index">
        <div class="intro"></div>
  <p class="intro_text">Welcome to Web Bug Tracker; the place for registering bugs and enhancements against the Symbian Foundation website content, functionality or access. </p>
  <p class="intro_text"><strong>Note*</strong> To raise bugs against the Symbian Foundation platform/code packages, please go to our <a href="/bugs">bug tracker</a>.</p>
  <p class="intro_text">To see what's new in this version
    of Web Bug Tracker, see the 
    <a href="page.cgi?id=release-notes.html">release notes</a>!
    You may also want to read the 
    <a href="docs/en/html/using.html">Web Bug Tracker User's Guide</a> to find out more about 
    Web Bug Tracker and how to use it.</p>

  <p>Most common actions:</p>
  <ul id="common_actions">
    <li id="query"><img height="9" width="5" alt="" src="/img/btn_cta.gif"/>&nbsp;<a href="query.cgi">Search existing bug reports</a></li>
    <li id="enter-bug"><img height="9" width="5" alt="" src="/img/btn_cta.gif"/>&nbsp;<a href="enter_bug.cgi">Enter a new bug report</a></li>
    <li id="report"><img height="9" width="5" alt="" src="/img/btn_cta.gif"/>&nbsp;<a href="report.cgi">Summary reports and charts</a></li>
            </ul>
            <ul class="additional_links">

    <li id="sidebar"><a href="javascript:addSidebar()">Add to Sidebar</a> (requires a Mozilla browser like Mozilla Firefox)</li>
    <li id="quick_search_plugin">
      <a href="javascript:window.external.AddSearchProvider('http://developer.symbian.org/webbugs/search_plugin.cgi')">Install
      the Quick Search plugin</a> (requires Firefox 2 or Internet Explorer 7)
    </li>

  </ul>


